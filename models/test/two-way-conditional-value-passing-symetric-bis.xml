<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>const int MAX = 10;
const int MIN = 0;
chan SEND[MAX-MIN+1];</declaration>
	<template>
		<name x="5" y="5">P0</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-501" y="-76">
			<name x="-484" y="-85">sent</name>
		</location>
		<location id="id1" x="-493" y="-195">
		</location>
		<init ref="id1"/>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-739" y="-145">SEND[3-MIN]!</label>
			<nail x="-654" y="-187"/>
			<nail x="-645" y="-77"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-595" y="-144">SEND[2-MIN]!</label>
			<nail x="-603" y="-161"/>
			<nail x="-603" y="-102"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="select" x="-484" y="-161">i : int[MIN,MAX]</label>
			<label kind="synchronisation" x="-484" y="-144">SEND[i]!</label>
		</transition>
	</template>
	<template>
		<name>P1</name>
		<declaration>int value;</declaration>
		<location id="id2" x="-212" y="127">
		</location>
		<location id="id3" x="0" y="0">
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id3"/>
			<target ref="id2"/>
			<label kind="select" x="-59" y="42">i : int[MIN,MAX]</label>
			<label kind="synchronisation" x="-93" y="59">SEND[i-MIN]?</label>
			<label kind="assignment" x="-102" y="76">value := i</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
p0 = P0();
p1 = P1();
// List one or more processes to be composed into a system.
system p0, p1;
    </system>
	<queries>
	</queries>
</nta>
