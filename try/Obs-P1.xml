<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.

clock x;
chan reset;</declaration>
	<template>
		<name x="5" y="5">Obs</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-790" y="-484">
			<name x="-800" y="-518">Taken</name>
			<committed/>
		</location>
		<location id="id1" x="-1207" y="-552">
			<name x="-1217" y="-586">Idle</name>
		</location>
		<init ref="id1"/>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="-1088" y="-391">x := 0</label>
			<nail x="-994" y="-391"/>
			<nail x="-1130" y="-425"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-1003" y="-561">reset?</label>
			<nail x="-1071" y="-527"/>
		</transition>
	</template>
	<template>
		<name>P1</name>
		<location id="id2" x="-3102" y="-2686">
			<name x="-3112" y="-2720">loop</name>
			<label kind="invariant" x="-3112" y="-2669">x &lt;= 3</label>
		</location>
		<init ref="id2"/>
		<transition>
			<source ref="id2"/>
			<target ref="id2"/>
			<label kind="guard" x="-3084" y="-2792">x &gt;= 2</label>
			<label kind="synchronisation" x="-3084" y="-2775">reset!</label>
			<nail x="-2907" y="-2830"/>
			<nail x="-2898" y="-2626"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
ProcessObs = Obs();
ProcessP1 = P1();
// List one or more processes to be composed into a system.
system ProcessObs, ProcessP1;

    </system>
	<queries>
	</queries>
</nta>
